module Main.Com.Banking.Account.Model where

import Main.Com.Banking.Bank.Model qualified as Bank

type T = Account
type K = (Text, Bank.K)
type C = ContractId Account

template Account
  with
    owner: Party
    name: Text
    accountNumber: Text
    initialAmount: Decimal
    bankId: Bank.K
  where
    signatory owner
    key (owner, (accountNumber, bankId)) : (Party, K)
    maintainer key._1