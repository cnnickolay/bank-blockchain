module Main.Com.Banking.Bank.Controller where

import Main.Com.Banking.Account.Service qualified as Account
import Main.Com.Banking.Bank.Model qualified as Bank

template Controller
  with
    owner: Party
    bankId: Bank.K
  where
    signatory owner

    controller owner can
      nonconsuming CreateAccount : Account.CreatedAccount
        with command : Account.AccountCreate
        do Account.createAccount command
