module Test.Com.Banking.Fixtures where

import Main.Com.Banking.Bank.Service qualified as Bank
import Main.Com.Banking.Account.Service qualified as Account

-- createData : Scenario ()
createData = do
    master <- getParty "master"
    bankOwner <- getParty "bankOwner"
    account1Owner <- getParty "account1Owner"
    account2Owner <- getParty "account2Owner"

    Bank.BankCreated bank <- submit master do
        Bank.createBank master $ Bank.CreateBank bankOwner "bank1" "0000000"

    (account1, account2) <- submit bankOwner do
        Account.AccountCreated account1 <- Account.createAccount bankOwner $ Account.AccountCreate account1Owner "account1" "111111" 10.0 bank.bic
        Account.AccountCreated account2 <- Account.createAccount bankOwner $ Account.AccountCreate account2Owner "account2" "222222" 10.0 bank.bic
        pure (account1, account2)

    pure (bank, (account1, account2))